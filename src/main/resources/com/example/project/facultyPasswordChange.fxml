<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FacultySceneControllers.FacultyProfilePasswordController">
   <children>
      <Button fx:id="logoutButton" layoutX="546.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      <Button fx:id="confirm" layoutX="249.0" layoutY="311.0" mnemonicParsing="false" onAction="#confirmChange" text="Confirm Change" />
      <TextField fx:id="field2" layoutX="225.0" layoutY="258.0" />
      <TextField fx:id="field1" layoutX="225.0" layoutY="187.0" />
      <Label layoutX="245.0" layoutY="154.0" text="Enter New Password" />
      <Label fx:id="passwordLabel" layoutX="252.0" layoutY="228.0" text="Re-enter Password" />
